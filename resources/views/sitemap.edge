<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"> 
  <url>
    <loc>{{appUrl}}</loc>
  </url>
  <url>
    <loc>{{`${appUrl}/blog`}}</loc>
  </url>
  @each(content in contents)
  <url>
    <loc>{{`${appUrl}/${content.slug}`}}</loc>
    <lastmod>{{content.updatedAt.toFormat('yyyy-MM-dd')}}</lastmod>
  </url>
  @endeach
</urlset>
